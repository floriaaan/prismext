{
  "name": "prismext",
  "version": "0.1.1",
  "description": "A plug'n'play backoffice for any NextJS app using Prisma",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsc-watch --onSuccess \"babel --config-file ./config/babel.config.js src --out-dir dist --extensions \".tsx,.ts,.js,.jsx\"\"",
    "test": "echo \"Error: no test specified\" && exit 0",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --ext .ts,.tsx .",
    "build": "pnpm tsc --project tsconfig.json && babel --config-file ./config/babel.config.js src --out-dir dist --extensions \".tsx,.ts,.js,.jsx\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "backoffice",
    "prisma",
    "nextjs",
    "plug'n'play"
  ],
  "author": "floriaaan",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.1",
    "@babel/plugin-proposal-optional-catch-binding": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.22.4",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/runtime": "^7.22.3",
    "@prisma/client": "^4.15.0",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "next": "^13.4.4",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "@prisma/client": "^4.15.0",
    "next": "^13.4.4",
    "prisma": "^4.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
